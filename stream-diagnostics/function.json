{
  "bindings": [
    {
      "type": "eventHubTrigger",
      "name": "d2cMessage",
      "direction": "in",
      "path": "stream-diagnostics",
      "connection": "DIAG_ENDPOINT",
      "consumerGroup": "$Default"
    },
    {
      "type": "table",
      "name": "latencyTable",
      "tableName": "StreamLatencyTable",
      "connection": "AzureWebJobsDashboard",
      "direction": "out"
    },
    {
      "type": "table",
      "name": "errorTable",
      "tableName": "StreamErrorTable",
      "connection": "AzureWebJobsDashboard",
      "direction": "out"
    }
  ],
  "disabled": false
}